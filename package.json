{
  "name": "daisy-web-app",
  "version": "2.0.0",
  "description": "Daisy web application.",
  "author": "Barnabas Lesti <barnabas.lesti@gmail.com>",
  "license": "ISC",
  "scripts": {
    "start:client": "vue-cli-service serve ./src/index.client --mode=development",
    "start:server": "cross-env NODE_ENV=development nodemon ./src/index.server --config ./nodemon.json",
    "start:server:prod": "cross-env NODE_ENV=production forever start ./src/index.server.js",
    "stop:server:prod": "forever stop ./src/index.server.js",
    "build:client": "vue-cli-service build ./src/index.client --mode development",
    "build:client:prod": "vue-cli-service build ./src/index.client --mode production",
    "test:client": "cross-env NODE_ENV=test cypress open --config-file ./test/client/cypress.json",
    "test:server": "cross-env NODE_ENV=test mocha --config ./test/server/.mocharc.js",
    "mock": "node ./scripts/mock",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "chart.js": "^2.9.3",
    "config": "^3.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^5.9.3",
    "require-yml": "^1.3.2",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.5.0",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.0",
    "vuex": "^3.1.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@mdi/js": "^4.9.95",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^6.0.3",
    "cypress": "4.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-cypress": "^2.9.0",
    "eslint-plugin-vue": "^5.0.0",
    "faker": "^4.1.0",
    "json-loader": "^0.5.7",
    "mocha": "^7.1.0",
    "mongodb-memory-server": "^6.3.2",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "typeface-roboto": "^0.0.75",
    "vue-cli-plugin-i18n": "^0.6.1",
    "vue-cli-plugin-vuetify": "^2.0.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0",
    "yaml-loader": "^0.5.0"
  }
}
